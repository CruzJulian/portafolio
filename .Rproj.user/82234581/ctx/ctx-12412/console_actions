        "\n[27] \"      pattern = \\\"rmd\\\"\"                                \n[28] \"      )\"                                                \n[29] \"    ) %>% \"                                             \n[30] \"  lapply(read_lines) %>% \"                              \n[31] \"  lapply(str_replace_all, \\\"# \\\", \\\"## \\\") -> childs\"   \n[32] \"\"                                                       \n[33] \"\"                                                       \n[34] \"```\"                                                    \n[35] \"\"                                                       ",
        "\n[36] \"\"                                                       \n[37] \"```{r}\"                                                 \n[38] \"set.seed(123456789)\"                                    \n[39] \"\"                                                       \n[40] \"emo::ji(\\\"mail\\\") -> emoji_mail\"                        \n[41] \"\"                                                       \n[42] \"```\"                                                    \n[43] \"\"                                                       \n[44] \"\"                                                       ",
